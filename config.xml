<?xml version="1.0" encoding="UTF-8"?>
<widget id="com.github.vasudeva-chaynes.svadhyaya" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Svadhyaya</name>
  <description>    SGS Datta Deepam flash card application; under development.  </description>
  <author email="svadhyaya.app@gmail.com"	  href="https://github.com/vasudeva-chaynes/Svadhyaya">Vasudeva (Chris Haynes</author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
</widget>
